{
  "manifest_version": 3,
  "name": "Console Rules",
  "version": "0.1.0",
  "description": "Save and run code snippets directly in the browser console. Supports English and Portuguese.",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Console Rules",
    "default_icon": {
      "16": "icon-16x16.png",
      "48": "icon-48x48.png",
      "128": "icon-128x128.png"
    }
  },
  "icons": {
    "16": "icon-16x16.png",
    "48": "icon-48x48.png",
    "128": "icon-128x128.png"
  }
}

